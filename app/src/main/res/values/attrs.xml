<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BannerView">
        <attr name="dotIndicatorNormal" format="reference"/>
        <attr name="dotIndicatorSelect" format="reference"/>
        <attr name="dotLocation" format="enum">
            <enum name="center" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="-1"/>
        </attr>
        <attr name="dotSize" format="dimension"/>
        <attr name="dotDistance" format="dimension"/>
        <attr name="bottomBackground" format="color"/>
        <attr name="aspectRatio" format="float"/>
        <attr name="isLooper" format="boolean"/>
        <attr name="hasIndication" format="boolean"/>
        <attr name="open_mz_mode" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="TrackTextIndicator">
        <attr name="visibleIndicatorNum" format="integer"/>
    </declare-styleable>
</resources>